<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.YoRHa.crm.workbench.dao.ContactsActivityRelationDao">

    <insert id="insertCaRel" parameterType="ContactsActivityRelation">
        insert into tbl_contacts_activity_relation(
            id,
            contactsId,
            activityId
        )values(
            #{id},
            #{contactsId},
            #{activityId}
        )
    </insert>
  	
</mapper>

































